@page "/talepfis_ekle/{talepLogRef:int?}"

<HeaderSearchBar header="header" ShowSearchSection="false"></HeaderSearchBar>

<TalepFisEkleGuncelleComponent OnVazgecClicked="goBackList" logRef="talepLogRef"></TalepFisEkleGuncelleComponent>

@code {

    [Inject]
    NavigationManager navigation { get; set; }

    [Parameter]
    public int? talepLogRef { get; set; }

    public HeaderBarSearch header = new HeaderBarSearch();

    protected override Task OnInitializedAsync()
    {
        header.HeaderText = "Talep Oluştur";
        return Task.CompletedTask;
    }

    public void goBackList()
    {
        navigation.NavigateTo("/talepfis_liste");
    }

}
