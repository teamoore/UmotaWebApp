@page "/talepdetay_ekle"

<HeaderSearchBar header="header" ShowSearchSection="false"></HeaderSearchBar>

<TalepDetayEkleGuncelleComponent OnVazgecClicked="goBackTeklifList"></TalepDetayEkleGuncelleComponent>

@code {

    [Inject]
    NavigationManager navigation { get; set; }

    public HeaderBarSearch header = new HeaderBarSearch();

    protected override Task OnInitializedAsync()
    {
        header.HeaderText = "Talep Oluştur";
        return Task.CompletedTask;
    }

    public void goBackTeklifList()
    {
        navigation.NavigateTo("/talepdetay_liste");
    }

}
